<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFrameworks>netstandard2.0</TargetFrameworks>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Version>0.1.0</Version>
        <Authors>DC</Authors>
        <Company>ESFA</Company>
        <PackageLicenseUrl>https://github.com/SkillsFundingAgency/DC-EAS-1819/blob/master/LICENSE</PackageLicenseUrl>
        <PackageProjectUrl>https://github.com/SkillsFundingAgency/DC-EAS-1819</PackageProjectUrl>
        <Description>EAS EF Model Project.</Description>
